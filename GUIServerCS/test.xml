                <Input>0002000210</Input>
                <Expect>Green LED will be turn off.</Expect>
                <Desc>Turn off Green LED</Desc>
            </TestCase>
            <TestCase tc="TC_0007">
                <Input>0002000220</Input>
                <Expect>Orange LED will be turn off.</Expect>
                <Desc>Turn off Orange LED</Desc>
            </TestCase>
            <TestCase tc="TC_0008">
